{
  "manifest_version": 3,
  "name": "VPN Checker Extension",
  "version": "1.0",
  "permissions": [
    "nativeMessaging",
    "declarativeNetRequest",
    "storage",
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "*://*/*"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "ruleset_1",
        "enabled": true,
        "path": "rules.json"
      }
    ]
  }
}